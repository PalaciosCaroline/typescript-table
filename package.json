{
  "name": "typescript-table",
  "version": "3.1.9",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PalaciosCaroline/typescript-table.git"
  },
  "dependencies": {
    "react-icons": "^4.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && tsc && npm run copy:css",
    "copy:css": "cpy \"./src/lib/styles/*.css\" \"./dist/styles\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "typescript-exportdata": "^1.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.38.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.0.0",
    "web-vitals": "^2.1.4"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!typescript-exportdata).+\\.js$"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "table",
    "typescript",
    "react",
    "datatable",
    "pagination",
    "sorting",
    "filtering",
    "managing columns",
    "eslint",
    "jest",
    "css3",
    "accessibility",
    "export data"
  ]
}
